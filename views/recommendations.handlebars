<div class="header">
    <a href="/"><img id="logo" src="/images/logo.png" alt=""></a>
    <form id="recommend-form" action="/url/" method="get">
        <input name="url" type="text" placeholder="SoundCloud Playlist or Profile URL" autofocus>
    </form>
    <a id="about" href="/about">About</a>
</div>
<div class="content">
    <ul>
<!--
		<li>
			<div class="notfound">Could not find playlist or profile at the given URL</div>
		</li>
-->
		{{#each tracks}}
			<li>
	            <iframe width="100%" height="150" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/{{this}}&amp;auto_play=false&amp;hide_related=true&amp;show_comments=false&amp;show_user=true&amp;show_reposts=false&amp;visual=true"></iframe>
			</li>
		{{/each}}
    </ul>
</div>
